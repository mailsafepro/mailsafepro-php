{
    "name": "mailsafepro/mailsafepro-php",
    "version": "1.0.0",
    "description": "PHP SDK for MailSafePro Email Validation API. Robust and secure email validation and verification with batch processing, breach detection, and JWT authentication. GDPR compliant.",
    "keywords": [
        "email",
        "validation",
        "verification",
        "mailsafepro",
        "email-validator",
        "smtp",
        "dns",
        "breach-detection",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/mailsafepro/mailsafepro-php",
    "license": "MIT",
    "authors": [
        {
            "name": "MailSafePro",
            "homepage": "https://github.com/mailsafepro"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "MailSafePro\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "MailSafePro\\Test\\" : "test/" }
    }
}
